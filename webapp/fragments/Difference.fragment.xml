<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
  <Dialog
      title="Oven Difference between Wonder and SAP"
      class="sapUiSizeCozy">
    <beginButton>
      <Button text="Close" press="onCloseDifference"/>
    </beginButton>
    <content>
      <Table id="OvenDifference"  fixedLayout="false"   inset="false" items="{ path: '/HTovendifferenceSet' }" >
        <headerToolbar>
          <Toolbar>
          <Title text="Oven Difference List" level="H2"/>
            <ToolbarSpacer/>
          </Toolbar>
        </headerToolbar>
        <columns>
          <Column width="12em">
            <Text text="Warehouse Number "/>
          </Column>
          <Column width="12em">
            <Text text="Storage Bin"/>
          </Column>
          <Column width="12em">
            <Text text="Oven Difference"/>
          </Column>
        </columns>
        <items>
          <ColumnListItem>
            <cells>
              <Text text="{Lgnum}" />
              <Text text="{Lgpla}" />
              <Text text="{ZusppOvenDiff}" />
            </cells>
          </ColumnListItem>
        </items>
      </Table>
    </content>
  </Dialog>
</core:FragmentDefinition>