<mvc:View height="100%" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
	controllerName="ey.meg.root.controller.Selection">
	<Page id="SelectionPage" floatingFooter="true" showHeader="true"
		title="Heat Treat Schedule">
		<content>
			<f:SimpleForm id="idWarehouseSelection" editable="false"
				layout="ResponsiveGridLayout"
				labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12"
				adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
				emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1"
				singleContainerFullSize="false">
				<f:content>
					<Label class="sapUiSmallMarginTop" text="Warehouse" required="true" design="Bold" />
					<Input id="IDwarehouse" type="Text" textFormatMode="KeyValue"
						placeholder="Enter Warehouse ..." showSuggestion="false" showValueHelp="true"
						valueHelpRequest="handlevaluehelpWarehouse" valueState="None" liveChange="handleLiveChange">
					</Input>
				</f:content>
			</f:SimpleForm>
			<HBox justifyContent="Center">
				<Button type="Accept" text="Submit" press="onSubmit" />
			</HBox>
		</content>
		<!--&nbsp;&nbsp;
		<footer>
			<OverflowToolbar class="toolbarStyle">
				<ToolbarSpacer />
				<Button type="Accept" text="Submit" press="onSubmit" />
			</OverflowToolbar>
		</footer>
		-->
	</Page>
</mvc:View>